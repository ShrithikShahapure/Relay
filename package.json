{
  "name": "relay",
  "description": "A free video conferencing app made with react, typescript, express, redis and agora sdk.",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"cd ./client && vite\" \"cd ./server/src && cross-env NODE_ENV=development nodemon index.ts\"",
    "build": "concurrently \"cd ./client && tsc && vite build\" \"cd ./server && tsc\"",
    "start": "cd ./server/dist && cross-env NODE_ENV=production node index.js",
    "preview": "npm run build && npm run start",
    "preview:client": "cd ./client && vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohitman47/spark-vc.git"
  },
  "keywords": [
    "video",
    "calling",
    "conferencing",
    "react",
    "express",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/rohitman47/spark-vc/issues"
  },
  "homepage": "https://github.com/rohitman47/spark-vc#readme",
  "author": "rohitman47",
  "license": "MIT",
  "dependencies": {
    "agora-access-token": "^2.0.4",
    "agora-rtc-sdk-ng": "^4.11.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fastify": "^4.5.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "redis": "^4.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.34",
    "@types/postcss-preset-env": "^7.7.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "concurrently": "^7.2.1",
    "cypress": "^10.6.0",
    "nodemon": "^2.0.16",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.6.0",
    "sass": "^1.51.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vite-plugin-pwa": "^0.12.3",
    "vite-plugin-svgr": "^2.1.0"
  }
}
